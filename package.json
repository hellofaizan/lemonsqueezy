{
  "name": "lmnsqz",
  "version": "0.1.0",
  "description": "Unofficial TypeScript SDK for the Lemon Squeezy API",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexgrozav/lmnsqz.git"
  },
  "homepage": "https://github.com/alexgrozav/lmnsqz",
  "bugs": "https://github.com/alexgrozav/lmnsqz/issues",
  "author": {
    "name": "alexgrozav",
    "email": "alex@grozav.com",
    "url": "https://grozav.com"
  },
  "keywords": [
    "api",
    "lemonsqueezy",
    "typescript"
  ],
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "files": [
    "**/*",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=16.*"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./checkout": {
      "import": "./modules/checkout.mjs",
      "require": "./modules/checkout.js"
    },
    "./customer": {
      "import": "./modules/customer.mjs",
      "require": "./modules/customer.js"
    },
    "./client": {
      "import": "./client/index.mjs",
      "require": "./client/index.js"
    },
    "./discount": {
      "import": "./modules/discount.mjs",
      "require": "./modules/discount.js"
    },
    "./file": {
      "import": "./modules/file.mjs",
      "require": "./modules/file.js"
    },
    "./licenseKey": {
      "import": "./modules/licenseKey.mjs",
      "require": "./modules/licenseKey.js"
    },
    "./licenseKeyInstance": {
      "import": "./modules/licenseKeyInstance.mjs",
      "require": "./modules/licenseKeyInstance.js"
    },
    "./modules": {
      "import": "./modules/index.mjs",
      "require": "./modules/index.js"
    },
    "./order": {
      "import": "./modules/order.mjs",
      "require": "./modules/order.js"
    },
    "./orderItem": {
      "import": "./modules/orderItem.mjs",
      "require": "./modules/orderItem.js"
    },
    "./product": {
      "import": "./modules/product.mjs",
      "require": "./modules/product.js"
    },
    "./store": {
      "import": "./modules/store.mjs",
      "require": "./modules/store.js"
    },
    "./subscription": {
      "import": "./modules/subscription.mjs",
      "require": "./modules/subscription.js"
    },
    "./subscriptionInvoice": {
      "import": "./modules/subscriptionInvoice.mjs",
      "require": "./modules/subscriptionInvoice.js"
    },
    "./types": {
      "import": "./types.mjs",
      "require": "./types.js"
    },
    "./user": {
      "import": "./modules/user.mjs",
      "require": "./modules/user.js"
    },
    "./variant": {
      "import": "./modules/variant.mjs",
      "require": "./modules/variant.js"
    }
  },
  "typesVersions": {
    "*": {
      "checkout": [
        "./modules/checkout.d.ts"
      ],
      "customer": [
        "./modules/customer.d.ts"
      ],
      "client": [
        "./client/index.d.ts"
      ],
      "discount": [
        "./modules/discount.d.ts"
      ],
      "file": [
        "./modules/file.d.ts"
      ],
      "licenseKey": [
        "./modules/licenseKey.d.ts"
      ],
      "licenseKeyInstance": [
        "./modules/licenseKeyInstance.d.ts"
      ],
      "modules": [
        "./modules/index.d.ts"
      ],
      "order": [
        "./modules/order.d.ts"
      ],
      "orderItem": [
        "./modules/orderItem.d.ts"
      ],
      "product": [
        "./modules/product.d.ts"
      ],
      "store": [
        "./modules/store.d.ts"
      ],
      "subscription": [
        "./modules/subscription.d.ts"
      ],
      "subscriptionInvoice": [
        "./modules/subscriptionInvoice.d.ts"
      ],
      "types": [
        "./types.d.ts"
      ],
      "user": [
        "./modules/user.d.ts"
      ],
      "variant": [
        "./modules/variant.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "npm run build --watch",
    "build": "rimraf lib && unbuild",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "format": "prettier --write \"**/*.{js,json,ts}\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest watch --ui",
    "prerelease": "npm run build",
    "release": "np --no-tests",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts"
  },
  "dependencies": {
    "qs": "^6.11.2",
    "undici": "^5.20.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^18.14.6",
    "@types/qs": "^6.9.10",
    "@types/shelljs": "^0.8.15",
    "@vitest/ui": "^0.29.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "minifaker": "^1.34.1",
    "np": "^9.1.0",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.0",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "unbuild": "^2.0.0",
    "vitest": "^0.29.2"
  }
}
